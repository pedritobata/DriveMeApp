{
  "expo": {
    "name": "DriveMeApp-Customer",
    "description": "Taxi Booking App",
    "slug": "DriveMeAppCustomer",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "notification": {
      "icon": "./assets/images/push_notification.png"
    },
    "version": "3.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pedromartinez.drivemeappcustomer",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app uses the location to serve you better",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyBwIFAFdg7-DIDZo-lPTwGYbCKLAf88Vtg"
      }
    },
    "android": {
      "package": "com.pedromartinez.drivemeappcustomer",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA_ROLL"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBwIFAFdg7-DIDZo-lPTwGYbCKLAf88Vtg"
        }
      }
    },
    "facebookScheme":"fbXXXXXXXXXXXXXX",
    "facebookAppId":"XXXXXXXXXXXXXX",
